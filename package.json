{
    "name": "node-pdf2img-monorepo",
    "version": "1.0.0",
    "private": true,
    "description": "PDF to image converter monorepo",
    "type": "module",
    "scripts": {
        "build": "pnpm -r build",
        "test": "pnpm -r test",
        "test:cli": "node packages/pdf2img/bin/cli.js",
        "benchmark": "node test/file-benchmark.mjs",
        "benchmark:stream": "node test/stream-benchmark.mjs",
        "publish:pdf2img": "cd packages/pdf2img && npm publish",
        "publish:pdf2img:dry": "cd packages/pdf2img && npm publish --dry-run",
        "clean": "rm -rf packages/*/node_modules node_modules"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "packageManager": "pnpm@8.0.0"
}
